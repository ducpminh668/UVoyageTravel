@using UvoyageTravel.Models.Entity;
@model List<UvoyageTravel.Models.Entity.Account>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";

}


<ul>
    @foreach (Account acc in Model)
    {
        <li>@acc.username
            <ul>
                @foreach(string role in acc.Roles)
                {
                    <li>@role</li>
                }
            </ul>
        </li>
    }
</ul>

